<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.wireguard.{{ item }}</string>
        <key>ProgramArguments</key>
        <array>
            <string>{{ wireguard_macos_wgquick_path }}</string>
            <string>up</string>
            <string>{{ wireguard_macos_remote_directory }}/{{ item }}.conf</string>
        </array>
        <key>OnDemand</key>
        <false/>
        <key>RunAtLoad</key>
        <true/>
        <key>TimeOut</key>
        <integer>90</integer>
        <key>EnvironmentVariables</key>
        <dict>
            <key>PATH</key>
            <string>{{ wireguard_macos_env_path }}</string>
        </dict>
    </dict>
</plist>
